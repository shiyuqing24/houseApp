body
  margin 0
  padding 0
  background #efeff4
  font-size 14px
ul
  margin 0
  padding 0
  list-style none
header
  height 35px
  background #ffffff
  line-height 35px
  text-align center
  font-size 18px
  border-bottom  1px solid #d6d7dc
  .con
    padding 0 3%
    span
      font-size 14px
      font-weight 400
      &:nth-child(1)
        float left
      &:nth-child(2)
        float right
        color #ea5404
article
  padding 70px 20%
  text-align center
  color #b8b8b9
  .box
    width 100%
    height 45px
    color #ffffff
    background url("../../image/publish/content_plus_icon@2x.png") no-repeat center #878787
    background-size 45px 45px
    margin 22px 0
    padding-top 100px
    position relative
    input
      display block
      width 100%
      height 100%
      position absolute
      left 0
      top 0
      opacity 0
.hid
  display none
  position fixed
  top 0
  bottom 0
  width 100%
  background rgba(0,0,0,0.2)
  .box
    width 72%
    height 110px
    background #ffffff
    border-radius 10px
    position absolute
    left 14%
    top 50%
    margin-top -55px
    .text
      display inline-block
      width 100%
      height 66px
      line-height 66px
      text-align center
      border-bottom 1px solid #d6d7dc
    .btn
      color #ea5404
      height 40px
      line-height 40px
      width 48%
      border none
      outline none
      border-radius 5px 0 0 5px
      background-color #ffffff
      &.cancel
        color #323232
    .confirms
      margin-left 26%