$orange=#ed692b
*
  outline none/*outline-style 属性用于设置元素的整个轮廓的样式,none定义无轮廓*/
body
  margin 0
  padding 0
  background-color #EFEFEF
  font-family SimHei
.header
  height 35px
  background-color #fff
  line-height 35px
  .icon-back
    position absolute
    top 0
    left 10px
    display inline-block
    width 35px
    height 35px
    line-height 35px
    background-size 10px 16px
  .header-title
    text-align center
    display inline-block
    width 100%
.search_box
  margin-top 10px
  font-size 0.8rem
  padding 0px 10px
  position relative
  .search_inp
    width 60%
    height 30px
    border-radius 3px
    border none
    border 1px solid #c5c5c6
    padding-left 40px

  &:before
    content ''
    width 26px
    height 26px
    background url("../../image/publish/content_search_icon@2x.png") no-repeat left center
    background-size 13px
    position absolute
    top 3px
    left 25px
  .search_right
    right 15px
    position absolute
    top 10px

    &:after
      content ''
      width 24px
      height 24px
      background url("../../image/publish/content_plus_icon@2x.png") no-repeat left center
      background-size 12px
      position absolute
      top -5px
      left -20px

.form_cells
  margin-top 10px
  height 350px
  background-color #FFFFFF
  padding 10px 30px
  .form_cell
    position relative
    &:before
      content ''
      width 6px
      height 6px
      background-color #BFE94C
      border-radius 50%
      position absolute
      top 5px
      left -15px
    .form_list
      margin 0px
      list-style none
      padding 8px 10px
      position relative
      .form_li
        padding 7px 0px
        position relative
        &:after
            content ''
            width 100%
            height 1px
            background-color #c5c5c6
            position absolute
            top 30px
            left 40px
      .form_li1
        margin-left -30px
        padding 7px 0px
        position relative
        &:after
          content ''
          width 100%
          height 1px
          background-color #c5c5c6
          position absolute
          top 30px
          left 70px
      .form_inp
        border none
        font-size 0.8rem
        line-height 10px
.form_cells1
  height 1000px
  margin-top 10px
  background-color #FFFFFF
  padding 10px 30px
  .form_cell1
    position relative
    &:before
      content ''
      width 6px
      height 6px
      background-color #5BD2EB
      border-radius 50%
      position absolute
      top 5px
      left -15px
    .form_txt
      padding 7px 20px
      font-size 0.9rem
  .radio_box
    position relative
    &:after
      content ''
      width 120%
      height 1px
      background-color #c5c5c6
      position absolute
      top 80px
      left -30px
    .radio_block
      display inline-block
      height 24px
      line-height 24px
      font-size 0.7rem
      padding-left 30px

      .radio_btn
        width 12px
        height 12px
        border 1px solid #EA5404
        border-radius 50%
        display inline-block
        margin 5px
        vertical-align bottom
      &.selected
        .icon
          width 4px
          height 4px
          border 1px solid transparent
          border-radius 50%
          background-color #EA5404
          display inline-block
          margin 3px
  .select_form
    .select_form_list
      padding 2px
      list-style none
      margin-left -18px
      font-size 1rem
      color #B4B4B4
      .select_form_li
        margin 5px
       .select_form_title
         padding-right 10px
         font-size 0.8rem
       .select_form_inp
         padding 8px
         border none
         height 20px
         width 140px
         background-color #efefef
         border-radius 5px
         font-size 0.8rem

       .select_form_last
         padding 0px 8px
         color #0f0f0f
         font-size 0.8rem
      .select_form_li1
        margin 3px
        position relative
        &:after
          content ''
          width 28px
          height 16px
          background url("../../image/publish/content_list_icon01@3x.png")no-repeat right  center
          background-size 14px 8px
          position absolute
          top 15px
          right 30px
        .select_form_title1
          padding-right 2px
          font-size 0.8rem
        .select-title
          border none
          padding 5px
          border none
          height 25px
          width 220px
          background-color #efefef
          border-radius 5px
          font-size 0.8rem
          position relative
          left 8px
        .select-list
          color #0f0f0f
          display none
          list-style none
          font-size 0.9rem
          position absolute
          width 195px
          right 13px
          border-radius  0 0 5px 5px
          top 38px
          background-color #efefef
          .select-li
            height 40px
            margin-left -35px
            line-height 40px
            position relative
            &:after
              content ''
              height 1px
              background-color #cecece
              position absolute
              display block
              bottom 0px
              left 0px
              width 220px
            &:last-child:after
              display none
.form_cells1
  .form_cell1
    .select_form
      .select_form_list
        .select_form_li
          position relative
          .radio_btn
            position absolute
            top 7px
            width 12px
            height 12px
            border 1px solid #EA5404
            border-radius 50%
            display inline-block
            margin 4px
            vertical-align bottom
            right 70px
          .icon
            position absolute
            top 0px
            width 4px
            height 4px
            border 1px solid transparent
            border-radius 50%
            background-color #EA5404
            display inline-block
            margin 3px

          .select_form_last1
            padding 0px 12px
            color #0f0f0f
            font-size 0.8rem
        .select_form_li2
          margin 5px
          position relative

          .select_form_title1
            padding-right 2px
            font-size 0.8rem
          .select-title
            border none
            padding 5px
            border none
            height 25px
            width 220px
            background-color #efefef
            border-radius 5px
            font-size 0.8rem
            position relative
      .select_form_li2
        position relative
        .radio_btn
          position absolute
          top 7px
          width 8px
          height 8px
          border 1px solid #EA5404
          border-radius 50%
          display inline-block
          margin 4px
          vertical-align bottom
          right 77px
        .select_form_last1
          padding 0px 10px
          color #0f0f0f
          font-size 0.8rem
      .select_form_li2
        position relative
        .select_form_inp1
          padding 8px
          border none
          height 20px
          width 95px
          background-color #efefef
          border-radius 5px
          font-size 0.8rem
        .select-txt
          display inline-block
          width 210px
          height 100px
          background-color #efefef
          padding 10px
          border-radius 5px
          font-size 0.8rem
    .footer
      padding 15px
      .footer_img
        width 100px
        height 100px
        background-color #efefef
        position relative
        &:before
        &:after
          content ''
          position absolute
          top 36px
          left 36px
          width 25px
          height 2px
          background-color #6c6c6c
          transform rotate(90deg)
        &:after
          right 15px
          transform rotate(180deg)
        .footer_txt
          font-size 0.8rem
          position absolute
          bottom  30px
          left 22px
          color #6c6c6c


























