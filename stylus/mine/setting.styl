body
  margin 0
  padding 0
  background #efefef
  font-size 14px
header
  height 35px
  background #ffffff
  line-height 35px
  .icon-back
    position absolute
    top 2px
    left 5px
    display inline-block width 30px
    width 30px
    height 30px
    background url("../../image/person/nav_icon_back@2x.png") no-repeat center
    background-size 12px 18px
  .header_title
    display inline-block
    width 100%
    text-align center
    font-size 20px
.list,.list1,.outLogin
  background #ffffff
  margin-top 10px
  height 30px
  line-height 30px
  padding 10px
  position relative
  .box
    width 50px
    height 25px
    border-radius 20px
    background #ea5404
    position absolute
    right 10px
    top 10px
    .circle
      width 25px
      height 25px
      border-radius 50%
      background #ffffff
      position absolute
      left 0
      top 0
      box-shadow inset -2px 0 2px
    .active
      width 25px
      height 25px
      border-radius 50%
      background #ffffff
      position absolute
      right 0
      top 0
      box-shadow inset 2px 0 2px
.list1
  position relative
  &:after
    content ''
    width 20px
    height 15px
    display block
    background url("../../img/my/content_list_right_icon@2x.png") no-repeat center
    position absolute
    right 10px
    top 17px
  .count
    position absolute
    right 40px
    color #999
.outLogin
  margin-top 20px
  text-align center
.hid
  display none
  position fixed
  top 0
  bottom 0
  width 100%
  background rgba(0,0,0,0.4)
  .box
    width 72%
    height 110px
    background #ffffff
    border-radius 10px
    position absolute
    left 14%
    top 50%
    margin-top -55px
    .text
      display inline-block
      width 100%
      height 66px
      line-height 66px
      text-align center
      border-bottom 1px solid #d6d7dc
    .btn
      color #ea5404
      height 40px
      line-height 40px
      width 48%
      border none
      outline none
      border-radius 5px 0 0 5px
      background-color #ffffff
    .btn1
      color #999
      border-right 1px solid #d6d6dc
