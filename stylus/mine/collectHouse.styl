body
  margin 0
  padding 0
  background #efefef
  font-size 14px

header
  height 35px
  background #ffffff
  line-height 35px
  .icon-back
    position absolute
    top 2px
    left 5px
    display inline-block
    width 30px
    height 30px
    background url("../../image/person/nav_back_icon@2x.png") no-repeat center
    background-size 12px 18px
  .header_title
    display inline-block
    width 100%
    text-align center
    font-size 20px
.title
  width 100%
  height 40px
  background #ffffff
  line-height 40px
  margin-top 10px
  div
    float left
    width 49%
    height 34px
    margin-top 3px
    text-align center
    padding 10px auto
    color #999
    &.t1
      span
        display block
        width 100%
        height 28px
        border-right 1px solid #d6d7dc
      &.active
        color tomato
        border-bottom 1px solid tomato
    &.house
      span
        display block
        width 100%
        height 28px
        border-left 1px solid #d6d7dc
      &.active
        color tomato
        border-bottom 1px solid tomato

.demand
  padding 10px
  .list
    height 115px
    padding 10px
    background #ffffff
    margin-bottom 10px
    .list-title
      font-size 18px
      font-weight 500
      padding 10px
      margin-bottom 10px
      background url("../../image/msg/content_list_border.9@2x.png") no-repeat 0 bottom
      .list-right
        float right
        color #ffffff
        background #15b755
        border-radius 5px
        padding 2px 5px
        font-size 12px
    .botton
      text-align center
      padding  10px
      button
        background #efeff4
        color #646464
        padding 5px
        border-radius 5px
        border none
      span
        display block
        float right
    &:nth-child(2)
      .list-right
        background #0193e6
.listing
  padding 10px
  display none
  .list
    height 115px
    padding 10px
    background #ffffff
    margin-bottom 10px
    position relative
    .icon-back
      display block
      width 34%
      margin-right 6%
      height 110px
      float left
      background url("../../image/index/content_img_list@2x.png") no-repeat center
      background-size 130px 110px
    .info
      float left
      width 53%
      height 100px
      line-height 25px
      font-size 12px
      .title
        display block
        font-size 18px
        font-weight bold
        margin-top 0
      .add
        font-size 16px
      button
        color tomato
        background #ffffff
        border 1px solid tomato
        border-radius 5px
      .price
        color tomato
        font-size 16px
      .date
        display inline-block
        position absolute
        right 5px
        bottom 6px






